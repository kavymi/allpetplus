{
  "name": "pet-licensing",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/pet-licensing/src",
  "projectType": "application",
  "tags": [
    "type:app",
    "scope:frontend",
    "domain:pet-licensing"
  ],
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vite --port 3001",
        "cwd": "apps/pet-licensing",
        "color": true
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "outputs": [
        "{workspaceRoot}/dist/apps/pet-licensing/client"
      ],
      "defaultConfiguration": "production",
      "options": {
        "command": "vite build",
        "cwd": "apps/pet-licensing"
      },
      "configurations": {
        "development": {
          "command": "vite build --mode development",
          "cwd": "apps/pet-licensing"
        },
        "production": {
          "command": "vite build --mode production",
          "cwd": "apps/pet-licensing"
        }
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vite preview --port 3001",
        "cwd": "apps/pet-licensing"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/pet-licensing/**/*.{ts,tsx}"
        ]
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -p apps/pet-licensing/tsconfig.json --noEmit"
      }
    }
  }
}
