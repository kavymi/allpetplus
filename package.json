{
  "name": "@pet/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "npm run db:generate && npm run ports:kill && nx run-many -t dev --max-parallel=6",
    "dev:all": "nx run-many -t dev --parallel=6",
    "dev:frontend": "nx run-many -t dev --projects=tag:scope:frontend --parallel=3",
    "dev:backend-all": "nx run-many -t serve --projects=tag:scope:backend --parallel=3",
    "dev:web": "nx dev web",
    "dev:backend": "nx dev backend",
    "dev:builder": "nx dev builder-service",
    "dev:licensing": "nx dev pet-licensing",
    "db:generate": "nx run-many -t db:generate --parallel",
    "start": "nx run-many -t dev --parallel",
    "start:fast": "nx run-many -t dev --parallel --skip-nx-cache",
    "build": "nx run-many --target=build --all --parallel=3",
    "build:affected": "nx affected --target=build --parallel=3",
    "lint": "nx run-many --target=lint --all --parallel=3",
    "lint:affected": "nx affected --target=lint --parallel=3",
    "test": "nx run-many --target=test --all --parallel=3",
    "test:affected": "nx affected --target=test --parallel=3",
    "test:watch": "nx run-many --target=test --all --watch",
    "e2e": "nx run-many --target=e2e --all",
    "typecheck": "nx run-many --target=typecheck --all",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "nx": "nx",
    "affected": "nx affected",
    "graph": "nx graph",
    "clean": "nx reset && rm -rf dist .nx/cache node_modules/.cache",
    "prepare": "husky || true",
    "analyze": "nx run-many --target=analyze --all",
    "workspace:lint": "nx workspace-lint",
    "workspace:update": "nx migrate latest",
    "ci": "npm run lint && npm run typecheck && npm run test && npm run build",
    "scaffold": "./scripts/scaffold.sh",
    "ports:check": "./scripts/check-ports.sh",
    "ports:check:verbose": "./scripts/check-ports.sh --verbose",
    "ports:kill": "./scripts/check-ports.sh --kill",
    "ports:kill:verbose": "./scripts/check-ports.sh --kill --verbose",
    "ports:status": "./scripts/check-ports.sh --extra --verbose",
    "deploy:staging": "gh workflow run deploy.yml --ref main --inputs environment=staging",
    "deploy:production": "gh workflow run deploy.yml --ref production --inputs environment=production"
  },
  "private": true,
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/compat": "^1.1.1",
    "@eslint/eslintrc": "^2.1.1",
    "@nx/cypress": "^20.3.0",
    "@nx/devkit": "20.8.2",
    "@nx/eslint": "^20.3.0",
    "@nx/eslint-plugin": "^21.6.3",
    "@nx/jest": "20.8.2",
    "@nx/js": "20.8.2",
    "@nx/node": "20.8.2",
    "@nx/playwright": "20.8.2",
    "@nx/vite": "^20.3.0",
    "@nx/workspace": "^20.3.0",
    "@playwright/test": "^1.36.0",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.6.0",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@tanstack/router-devtools": "^1.96.0",
    "@tanstack/router-plugin": "^1.96.0",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@types/jest": "^29.5.12",
    "@types/node": "18.16.9",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "cypress": "^13.13.2",
    "dotenv-cli": "^7.4.2",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.1",
    "eslint-plugin-playwright": "^1.6.2",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "5.0.0",
    "husky": "^9.1.4",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jsonc-eslint-parser": "^2.1.0",
    "lint-staged": "^15.2.7",
    "nx": "^20.3.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.3",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "typescript": "^5.7.2",
    "vite": "^6.0.11",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "workspaces": [
    "apps/*",
    "services/*",
    "libs/*",
    "tools/*"
  ],
  "packageManager": "npm@10.7.0",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "keywords": [],
  "author": "",
  "description": "All Pet Plus - E-commerce platform for customizable pet products",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "nx affected --target=lint --uncommitted --fix",
      "nx affected --target=test --uncommitted"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "dependencies": {
    "@clerk/clerk-react": "^5.20.0",
    "@fontsource/inter": "^5.1.0",
    "@fontsource/jetbrains-mono": "^5.1.1",
    "@fontsource/plus-jakarta-sans": "^5.1.0",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-router": "^1.96.0",
    "@trpc/client": "^11.6.0",
    "@trpc/react-query": "^11.6.0",
    "@trpc/server": "^11.6.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-helmet-async": "^2.0.5",
    "superjson": "^2.2.2"
  },
  "overrides": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }
}